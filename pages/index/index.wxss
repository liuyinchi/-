view{
  color: var(--themeColor);
}

.clearfix:after {
  content: ".";   
  display: block; 
  clear: both;    
  visibility: hidden;  
  height: 0;   
  font-size:0;  
}

.index_swiper swiper{
  width: 750rpx;
  height: 340rpx;
}

.index_swiper swiper image{
  width: 100%;
}

.index_cate{
  display: flex;
}

.index_cate navigator{
  padding: 20rpx;
  flex: 1;
}

.index_cate navigator image{
  width: 100%;
}

.index_floor{

}

.index_group{

}

.floor_title{
  padding: 10rpx 0;
}

.floor_title image{
  width: 100%;
}

.floor_list{

}

.floor_list navigator{
  float: left;
  width: 33.33%;
}

.floor_list navigator:first-child{
  height: 55.45422414vw;
}

.floor_list navigator:nth-last-child(-n+4){
  height: 27.72711207vw;
}

.floor_list navigator image{
  width: 100%;
  height: 100%;
}

.floor_list navigator:nth-last-child(-n+4) image{
  padding-left: 10rpx;
}

.floor_list navigator:nth-last-child(-n+2){
  padding-top: 10rpx;
}